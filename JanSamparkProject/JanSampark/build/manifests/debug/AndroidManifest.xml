<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="1" android:versionName="1.0" package="com.jansampark.vashisthg">

    <uses-sdk android:minSdkVersion="10" android:targetSdkVersion="16"/>

    <permission android:name="com.jansampark.vashisthg.permission.MAPS_RECEIVE" android:protectionLevel="signature"/>

    <uses-permission android:name="com.citibikenyc.citibike.permission.MAPS_RECEIVE"/>

    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES"/>

    <!-- External storage for caching. -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <!-- My Location -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <!-- Maps API needs OpenGL ES 2.0. -->
    <uses-feature android:glEsVersion="0x00020000" android:required="true"/>

    <application android:allowBackup="true" android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:theme="@style/AppTheme">

        <meta-data android:name="com.google.android.maps.v2.API_KEY" android:value="AIzaSyCaSCB0RnzRz7J93mtXTwsr7vR3uvb-xKU"/>
        <activity android:label="@string/app_name" android:name="com.jansampark.vashisthg.SplashActivity" android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/title_activity_main" android:name="com.jansampark.vashisthg.MainActivity">
        </activity>
        <activity android:label="@string/title_activity_issue" android:name="com.jansampark.vashisthg.IssueActivity" android:parentActivityName="MainActivity">
            <meta-data android:name="android.support.PARENT_ACTIVITY" android:value="MainActivity"/>
        </activity>
    </application>

</manifest>
